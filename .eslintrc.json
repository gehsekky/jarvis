{
    "extends": "airbnb-base",
    "env": {
        "jest": true
    },
    "rules": {
        "class-methods-use-this": 0,
        "comma-dangle": 0,
        "global-require": 0,
        "import/no-dynamic-require": 0,
        "import/no-unresolved": 0,
        "indent": 0,
        "linebreak-style": 0,
        "no-console": 0,
        "no-empty-function": 0,
        "no-nested-ternary": 0,
        "no-param-reassign": 0,
        "no-restricted-syntax": 0,
        "no-useless-constructor": 0,
        "no-useless-escape": 0,
        "prefer-destructuring": 0,
        "semi": 0
    }
}