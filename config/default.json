{
  "slack-api-token": "your-api-token-goes-here",
  "auto-reconnect": true,
  "auto-mark": true,
  "command-prefix": ".",
  "modules": {
    "dongs": {},
    "giphy": {
      "api-key": "your-api-key-goes-here",
      "attachment-color": "#439FE0"
    },
    "imgur": {
      "clientId": "your-client-id-goes-here",
      "attachment-color": "#439FE0"
    },
    "wunderground": {
      "api-key": "your-api-key-goes-here"
    },
    "amazon": {
      "aws-access-key-id": "your-access-key-id-goes-here",
      "aws-secret-key": "your-secret-key-goes-here",
      "aws-tag": "your-amazon-associate-tag-goes-here",
      "attachment-color": "#764FA5"
    },
    "uptime": {}
  },
  "log4js": {
    "appenders": {
      "console": {
        "type": "console"
      },
      "app": {
        "type": "dateFile",
        "filename": "logs/jarvis",
        "pattern": "-yyyyMMdd.log",
        "alwaysIncludePattern": true
      }
    },
    "categories": {
      "default": {
        "appenders": [ "app" ],
        "level": "DEBUG"
      }
    },
    "replaceConsole": true
  }
}
